# proto-file: google3/third_party/openconfig/kne/proto/topo.proto
#  proto-message: Topology

name: "nikhil"
nodes: {
    name: "dut"
    type: ARISTA_CEOS
    vendor: ARISTA
    model: "ceos"
    os: "eos"
    config: {
        image: "ceos:latest"
        init_image: "us-west1-docker.pkg.dev/gep-kne/kne/networkop/init-wait:ga"
        config_path: "/mnt/flash"
        config_file: "startup-config"
        file: "arista.config"
        cert: {
            self_signed: {
                cert_name: "gnmiCert.pem",
                key_name: "gnmiCertKey.pem",
                key_size: 4096,
            }
        }
    }
}

nodes: {
    name: "otg"
    type: IXIA_TG
    version: "0.0.1-9999"
    services: {
        key: 443
        value: {
            name: "https"
            inside: 443
        }
    }
    services: {
        key: 40051
        value: {
            name: "grpc"
            inside: 40051
        }
    }
    services: {
        key: 50051
        value: {
            name: "gnmi"
            inside: 50051
        }
    }
}

links: {
    a_node: "dut"
    a_int: "eth1"
    z_node: "otg"
    z_int: "eth1"
}
links: {
    a_node: "dut"
    a_int: "eth2"
    z_node: "otg"
    z_int: "eth2"
}
links: {
    a_node: "dut"
    a_int: "eth3"
    z_node: "otg"
    z_int: "eth3"
}
links: {
    a_node: "dut"
    a_int: "eth4"
    z_node: "otg"
    z_int: "eth4"
}
